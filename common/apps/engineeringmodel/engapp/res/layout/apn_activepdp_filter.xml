<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        android:title="ApnActivepdpFilterSetting">

    <Preference
            android:key="networkinfo"
            android:title="NetworkConnection Info "
            android:persistent="false" />
            
    <PreferenceCategory
            android:key="ApnpdpFilterSelect"
            android:title="apn activepdp filter setting"
            android:persistent="false" >
    <CheckBoxPreference 
        android:key="all" 
        android:title="APN_TYPE_ALL"
        android:summary=""/>
        
    <CheckBoxPreference 
        android:key="default" 
        android:title="APN_TYPE_DEFAULT"
        android:summary=""/>
      
    <CheckBoxPreference 
        android:key="mms" 
        android:title="APN_TYPE_MMS"
        android:summary=""/>

    <CheckBoxPreference 
        android:key="supl" 
        android:title="APN_TYPE_SUPL" 
        android:summary=""/>
       
    <CheckBoxPreference 
        android:key="dun" 
        android:title="APN_TYPE_DUN"
        android:summary=""/>
        
    <CheckBoxPreference 
        android:key="hipri" 
        android:title="APN_TYPE_HIPRI"
        android:summary=""/>
        
  </PreferenceCategory>
</PreferenceScreen>